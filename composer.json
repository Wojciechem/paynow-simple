{
    "name": "wojciechem/paynow-simple",
    "description": "simple PHP client for paynow API",
    "type": "library",
    "require": {
        "php-http/client-implementation": "^1.0",
        "php-http/guzzle6-adapter": "^2.0",
        "ext-json": "*",
        "php": ">=7.1"
    },
    "require-dev": {
        "php": ">=7.4",
        "friendsofphp/php-cs-fixer": "^2.16",
        "php-http/mock-client": "^1.3",
        "phpunit/phpunit": "^9.1",
        "vimeo/psalm": "^3.11"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "wojciechem",
            "email": "wmiedzybrodzki@outlook.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "PaynowSimple\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PaynowSimple\\": "tests/"
        }
    }
}
